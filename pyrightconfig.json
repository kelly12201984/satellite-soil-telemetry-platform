{
  "include": [
    "api",
    "scripts"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.venv",
    "**/node_modules"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.13",
      "pythonPlatform": "All",
      "extraPaths": [
        ".",
        ".venv/Lib/site-packages",
        "api"
      ],
      "venvPath": ".",
      "venv": ".venv"
    }
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.13"
}